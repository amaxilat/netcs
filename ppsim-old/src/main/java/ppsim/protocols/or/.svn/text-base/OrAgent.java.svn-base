package ppsim.protocols.or;

import ppsim.model.AbstractAgent;
import ppsim.model.Population;

/**
 * Implements the agent of the Or protocol.
 */
public class OrAgent extends AbstractAgent<Boolean> {

    /**
     * Default constructor.
     *
     * @param pop the Population holder.
     */
    public OrAgent(final Population<Boolean> pop) {
        super(pop);
    }

    /**
     * Initializes the state of the agent.
     */
    public void initAgent() {
        setState(false);
    }

}
